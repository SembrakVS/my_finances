<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="vs_costs_form_view" model="ir.ui.view">
            <field name="name">vs_costs.form</field>
            <field name="model">vs_costs</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="create_date"/>
                            </group>
                            <group>
                                <field name="sum"/>
                                <field name="name"/>
                                <field name="bill"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="vs_costs_tree_view" model="ir.ui.view">
            <field name="name">vs_costs.tree</field>
            <field name="model">vs_costs</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="costs"/>
                    <field name="name"/>
                    <field name="sum"/>
                </tree>
            </field>
        </record>

