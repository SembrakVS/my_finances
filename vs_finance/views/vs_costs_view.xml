        <record id="vs_costs_act_window" model="ir.actions.act_window">
            <field name="name">ModelTitle</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vs_costs</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here to add new ModelTitle.
                </p>
            </field>
        </record>

        <menuitem name="Costs" id="vs_costs_menu" parent="costs_group_menu" action="vs_costs_act_window"/>

    </data>
</odoo>